//
// Double quote: \"
// Back slash: \\
// Newline: \n
// Tab: \t
//

var name = "John Doe"
var message1 = "Hello, \"${name}\""
print(message1) // Hello, "John Doe"

var message2 = "C:\\Windows\\Program Files"
print(message2) // C:\Windows\Program Files

var message3 = "\tLine 1\n\tLine 2\n\tLine 3"
//    Line 1
//    Line 2
//    Line 3
print(message3)
